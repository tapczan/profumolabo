{% extends '@PrestaShop/Admin/layout.html.twig' %}

{% block content %}
    <div class="row justify-content-center">
        <div class="col">
            <form method="post" class="form-horizontal">
                <div class="card">
                    <h3 class="card-header">
                        <i class="material-icons">settings</i>  Edit
                    </h3>
                    <div class="card-block row">
                        <div class="card-text">
                            <div class="row">
                                <div class="col-sm">

                                    {{ form(form) }}


                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="card-footer">
                        <div class="d-inline-flex">
                            <a class="btn btn-outline-secondary" href="{{ path('createit_custom_field') }}">Go back to custom field list</a>
                        </div>
                    </div>

                </div>
            </form>
        </div>
    </div>
{% endblock %}


{% block javascripts %}
    {{ parent() }}

{% endblock %}
